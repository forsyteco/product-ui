{
  "name": "@forsyteco/product-ui",
  "version": "0.0.1",
  "description": "Forsyte Product UI React component library",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./tailwind": "./tailwind.config.js"
  },
  "files": [
    "dist",
    "tailwind.config.js",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forsyteco/product-ui.git"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "tailwind"
  ],
  "author": "etiCloud Ltd",
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@tailwindcss/vite": "^4.1.17",
    "boring-avatars": "^2.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "lucide-react": "^0.556.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.1",
    "react-day-picker": "^9.12.0",
    "react-dom": "^19.2.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@storybook/addon-vitest": "^10.1.4",
    "@storybook/react-vite": "^10.1.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "19",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "jsdom": "^27.2.0",
    "playwright": "^1.57.0",
    "storybook": "^10.1.4",
    "tailwindcss": "^4.1.17",
    "vite": "^7.2.6",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^4.0.15"
  },
  "scripts": {
    "build": "vite build",
    "check-types": "tsc -p tsconfig.build.json --noEmit",
    "lint": "tsc -p tsconfig.build.json --noEmit",
    "test": "vitest run -c vitest.unit.config.ts",
    "test:cov": "vitest run -c vitest.unit.config.ts --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=19.2.1 <20",
    "react-dom": ">=19.2.1 <20",
    "tailwindcss": ">=4.1.17 <5"
  }
}
